from dtcc.core import *

c = Core()

# List projects and datasets
for project in c.GetProjects():
    print('Project: %s' % project)
for name in c.GetAvailableDataSetNames():
    print('Dataset: %s' % name)

# Generate data
c.GenerateDataSet('Majorna2021', 'GroundSurface')

# Get data
data = c.GetDataSet('Majorna2021', 'GroundSurface')
