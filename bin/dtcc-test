from dtcc.core import *

c = Core()

# List projects and datasets
for project in c.GetProjects():
    print('Project: %s' % project)
    for dataset in c.GetDataSets(project):
        print('  Dataset: %s' % dataset)

# Get data
data = c.GetData('Majorna2021', 'GroundSurface')
